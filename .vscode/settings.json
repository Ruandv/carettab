{
  "i18n-ally.localesPaths": ["./src/locales"],
  "i18n-ally.pathMatcher": "{locale}.json",
  "i18n-ally.defaultNamespace": "",
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.displayLanguage": "en",
  "i18n-ally.review.enabled": false,
  "i18n-ally.keystyle": "nested",
  "i18n-ally.enabledFrameworks": ["vue"],
  "i18n-ally.extract.keygenStyle": "camelCase",
  "i18n-ally.translate.engines": ["deepl", "google"],
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "stylelint.validate": ["css", "less", "postcss", "vue"],
  "json.schemas": [
    {
      "fileMatch": ["manifest.json"],
      "url": "https://json.schemastore.org/chrome-manifest.json"
    }
  ],
  "explorer.fileNesting.patterns": {
    "*Widget.vue": "$(capture).js, $(capture)*.vue, $(capture)*.scss, $(capture)*.css, $(capture)*.html",
    "*Tool.vue": "$(capture).js, $(capture)*.vue, $(capture)*.scss, $(capture)*.css, $(capture)*.html"
  }
}
